<% if params[:chart] == 'sprint' %>
  $('sprint_container').html('');
  var sprint_chart = set_data('sprint_container', <%= map_to_charts_series(@axes_sprint) %>, 'Sprint Burn Chart', 'Time (hrs)', 'hrs');
<% else %>
  $('release_container').html('');
  set_data('release_container', <%= map_to_charts_series(@axes_release) %>, 'Release Burnup Chart', 'Points (pts)', 'pts', {categories: <%=values_sorted_by_keys(@dates_map) %> });
<% end %>